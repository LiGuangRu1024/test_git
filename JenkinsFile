pipeline{
    agent any
    stages{
        stage("build"){
            steps{
                echo "Build"
                git 'git@github.com:LiGuangRu1024/test_git.git'
            }
        }
        stage("Test"){
            steps{
                echo "Test"
            }
        }
        stage("Deployment"){
            steps{
                echo "Deployment"
            }
        }
    }
     post{
        always{
            echo 'always say stop'
        }
    }
    parameters{
        string( name :'LiGuangRu1024',defaultValue:'test_git',description:'构建者')
    }
}
